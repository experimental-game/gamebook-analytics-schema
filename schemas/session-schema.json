{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "game": {
      "type": "string"
    },
    "user_id": {
      "type": "string"
    },
    "session_id": {
      "type": "string"
    },
    "timestamp": {
      "type": "integer"
    },
    "state": {
      "type": "string",
      "enum": [
        "START",
        "END"
      ]
    },
    "app_version": {
      "type": "string"
    }
  },
  "required": [
    "user_id",
    "session_id",
    "timestamp",
    "state"
  ]
}