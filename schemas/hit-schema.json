{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "workspace_id": {
      "type": "string"
    },
    "workspace_version": {
      "type": "string"
    },
    "node_id": {
      "type": "string"
    },
    "node_session_id": {
      "type": "string"
    },
    "device_id": {
      "type": "string"
    },
    "app_id": {
      "type": "string"
    },
    "session_id": {
      "type": "string"
    },
    "action": {
      "type": "string",
      "enum": [
        "NODE_ENTER",
        "NODE_LEAVE"
      ]
    },
    "platform": {
      "type": "string",
      "enum": [
        "ios",
        "android",
        "web"
      ]
    },
    "timestamp": {
      "type": "string",
      "format": "date-time"
    }
  },
  "required": [
    "workspace_id",
    "workspace_version",
    "node_id",
    "node_session_id",
    "device_id",
    "app_id",
    "session_id",
    "action",
    "platform",
    "timestamp"
  ]
}