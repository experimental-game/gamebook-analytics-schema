{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "game": {
      "type": "string"
    },
    "user_id": {
      "type": "string"
    },
    "country": {
      "type": "string"
    },
    "timestamp": {
      "type": "integer"
    },
    "platform": {
      "type": "string",
      "enum": [
        "IOS",
        "ANDROID",
        "WEB"
      ]
    },
    "ad_id": {
      "type": "string"
    },
    "email": {
      "type": "string"
    },
    "name": {
      "type": "string"
    },
    "facebook_id": {
      "type": "integer"
    },
    "os_version": {
      "type": "string"
    },
    "app_version": {
      "type": "string"
    },
    "adjust_id": {
      "type": "string"
    }
  },
  "required": [
    "user_id",
    "country",
    "timestamp",
    "platform"
  ]
}