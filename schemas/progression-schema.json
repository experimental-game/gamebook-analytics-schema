{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "game": {
      "type": "string"
    },
    "user_id": {
      "type": "string"
    },
    "session_id": {
      "type": "string"
    },
    "app_id": {
      "type": "string"
    },
    "device_id": {
      "type": "string"
    },
    "platform": {
      "type": "string",
      "enum": [
        "IOS",
        "ANDROID",
        "WEB"
      ]
    },
    "context_type": {
      "type": "string"
    },
    "context_name": {
      "type": "string"
    },
    "story_name": {
      "type": "string"
    },
    "episode_id": {
      "type": "string"
    },
    "build_number": {
      "type": "integer"
    },
    "timestamp": {
      "type": "integer"
    },
    "action_type": {
      "type": "string",
      "enum": [
        "NODE_LEAVE",
        "DECISION",
        "PREMIUM_DECISION",
        "PLAY",
        "LOAD"
      ]
    },
    "action_status": {
      "type": "string",
      "enum": [
        "NONE",
        "SELECT",
        "SKIP",
        "START",
        "RESTART",
        "RESUME",
        "QUIT",
        "COMPLETE"
      ]
    },
    "node_id": {
      "type": "string"
    },
    "trigger_id": {
      "type": "string"
    }
  },
  "required": [
    "game",
    "user_id",
    "session_id",
    "context_type",
    "context_name",
    "story_name",
    "timestamp",
    "action_type",
    "action_status"
  ]
}